pipeline {
  agent any
    stages {
      stage('Build') {
        steps {
	  echo 'building...'
	  bat 'mvn compile'
	  }
	}
	stage('Test') {
	  steps {
	    echo 'testing...'
	    bat 'mvn test'
	    }
	  }
	stage('Package') {
	  steps {
	    echo 'packaging...'
	    bat 'mvn install -DskipTests'
	    }
	  }
	stage('Archivage') {
	  steps {
	    echo 'archiving....'
	    archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true
	    }
	  }
	}
}

